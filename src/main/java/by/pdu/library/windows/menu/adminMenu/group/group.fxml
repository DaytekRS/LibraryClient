        </TableView>
        <Button layoutX="606.0" layoutY="38.0" mnemonicParsing="false" onAction="#addGroup" text="Button"/>
        <Button layoutX="606.0" layoutY="338.0" mnemonicParsing="false" onAction="#cancelButton" text="Button"/>
        <Button layoutX="606.0" layoutY="85.0" mnemonicParsing="false" onAction="#removeGroup" text="Button"/>
        <Button layoutX="606.0" layoutY="132.0" mnemonicParsing="false" onAction="#updateGroup" text="Button"/>
